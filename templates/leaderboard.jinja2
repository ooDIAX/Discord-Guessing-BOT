{%  extends "base.jinja2" %}

{% block content %}
    <h2 class="heading">Leaderboard</h2>

    <table class = table>
            <thead>
                <tr>
                    <!-- <th>ID</th> -->
                    <th>Name</th>
                    <th>Score</th>
                </tr>
    </thead>
    <tbody>

    {% for product in products %}
                <tr>
                    <td>{{ product[1] }}</td>
                    <td>{{ product[2] }}</td>
                </tr>
    {% endfor %}

    </tbody>
    </table>

    
{% endblock %}
